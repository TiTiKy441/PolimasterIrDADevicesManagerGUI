<Window x:Class="PolimasterIrDADevicesManagerGUI.GUI.RawIrDACommunicationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PolimasterIrDADevicesManagerGUI.GUI"
        mc:Ignorable="d"
        Title="IrDA communications" Height="323" Width="300" ResizeMode="NoResize">
    <Grid Margin="0,10,0,0">
        <ScrollViewer x:Name="responses_scrollviewer" Margin="0,0,0,26">
            <TextBlock x:Name="responses_textblock">
            </TextBlock>
        </ScrollViewer>
        <TextBox x:Name="send_textbox" Margin="0,248,0,0" KeyDown="send_textbox_KeyDown">
        </TextBox>
    </Grid>
</Window>
