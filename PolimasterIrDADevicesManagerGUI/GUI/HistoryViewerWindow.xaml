<Window x:Class="PolimasterIrDADevicesManagerGUI.GUI.HistoryViewerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PolimasterIrDADevicesManagerGUI.GUI"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="History viewer" Height="460" Width="450">
    <Grid>
        <ScrollViewer Margin="0,0,0,41">
            <StackPanel x:Name="history_stackpanel">
            </StackPanel>
        </ScrollViewer>
        <Border BorderBrush="Black" BorderThickness="1" Margin="0,381,0,0">
            <Grid>
                <Button x:Name="read_history_button" Content="Read history" Margin="9,9,299,8" Click="read_history_button_Click"/>
                <Button x:Name="plot_button" Content="Plot a graph" Margin="298,9,10,8" Click="plot_button_Click" IsEnabled="False"/>
                <Button x:Name="cancel_button" Content="Cancel read" Margin="154,9,155,8" Click="cancel_button_Click" IsEnabled="False"/>
            </Grid>
        </Border>
    </Grid>
</Window>
